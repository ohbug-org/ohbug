{
  "name": "@ohbug/webpack-plugin",
  "version": "1.0.13",
  "description": "Webpack plugin for upload your sourceMap files to Ohbug",
  "author": "chenyueban <jasonchan0527@gmail.com>",
  "homepage": "https://github.com/ohbug-org/ohbug",
  "bugs": {
    "url": "https://github.com/ohbug-org/ohbug/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ohbug-org/ohbug"
  },
  "license": "Apache-2.0",
  "main": "./dist/ohbug-webpack-plugin.umd.js",
  "module": "./dist/ohbug-webpack-plugin.es.js",
  "unpkg": "./dist/ohbug-webpack-plugin.umd.js",
  "jsdelivr": "./dist/ohbug-webpack-plugin.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ohbug-webpack-plugin.es.js",
      "require": "./dist/ohbug-webpack-plugin.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "buildOptions": {
    "name": "OhbugWebpackPlugin",
    "formats": [
      "es",
      "umd"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ohbug/cli": "^1.0.1",
    "@ohbug/types": "^1.1.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/multer": "^1.4.5",
    "@types/rimraf": "^3.0.2",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "rimraf": "^3.0.2",
    "webpack": "^5.52.0"
  }
}
