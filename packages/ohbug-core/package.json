  "homepage": "https://github.com/ohbug-org/ohbug",
  "bugs": {
    "url": "https://github.com/ohbug-org/ohbug/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ohbug-org/ohbug"
  },
  "license": "Apache-2.0",
  "main": "./dist/ohbug-core.umd.js",
  "module": "./dist/ohbug-core.es.js",
