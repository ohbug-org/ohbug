    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "dependencies": {
    "@ohbug/core": "workspace:*",
    "@ohbug/types": "workspace:*"
  },
  "devDependencies": {
    "@ohbug/browser": "workspace:*",
    "@types/react": "^18.0.9"
  },
  "publishConfig": {
    "access": "public"
  }
